<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.strangeye.dubalnebal.dao.PartyUserDao" >

    <insert id="participateParty" parameterType="com.strangeye.dubalnebal.dto.Party_User">
        INSERT INTO User_Party
        (
            User_user_id,
            Party_party_id
         )
        VALUES
        (
            #{user_id},
            #{party_id}
        )
    </insert>

</mapper>